{
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "L": true
  },
  "rules": {
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "space-unary-ops": 2,
    "no-use-before-define": [2, "nofunc"],
    "camelcase": 0,
    "comma-style": 2,
    "eqeqeq": 0,
    "new-cap": 2,
    "no-new": 2,
    "brace-style": 2,
    "no-multi-spaces": 0,
    "no-underscore-dangle": [0],
    "no-self-compare": 2,
    "no-void": 2,
    "no-eq-null": 2,
    "quotes": [0],
    "curly": 0,
    "dot-notation": [0],
    "no-shadow": 0,
    "no-alert": 0,
    "consistent-return": 0
  }
}